{
    "name":        "arcanedev/laravel-seo",
    "description": "SEO Package for Laravel.",
    "keywords":    ["arcanedev", "laravel", "seo"],
    "homepage":    "https://github.com/ARCANEDEV/LaravelSeo",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":               ">=5.6",
        "arcanedev/support": "~3.20"
    },
    "require-dev": {
        "phpunit/phpcov":  "~3.0",
        "phpunit/phpunit": "~5.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\LaravelSeo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\LaravelSeo\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.0\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
